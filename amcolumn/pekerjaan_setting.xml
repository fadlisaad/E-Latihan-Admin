<settings>
<data_type>csv</data_type>
<add_time_stamp>true</add_time_stamp>
<font>Tahoma</font>
<pie>
<x>200</x>
<base_color/>
<brightness_step>10</brightness_step>
<inner_radius>30</inner_radius>
<radius>90</radius>
<gradient>radial</gradient>
<gradient_ratio>0,0,-50,0,0,0,-50</gradient_ratio>
</pie>
<data_labels>
<radius/>
<show>{title}</show>
<line_color>#000000</line_color>
<line_alpha>15</line_alpha>
<hide_labels_percent/>
</data_labels>
<animation>
<start_time>2</start_time>
<start_effect>strong</start_effect>
<pull_out_time>1.5</pull_out_time>
<sequenced>true</sequenced>
</animation>
<group>
<percent>3</percent>
<color/>
<pull_out>true</pull_out>
<title>other</title>
</group>
<balloon>
<alpha>80</alpha>
<text_color>000000</text_color>
<show>{title}: {percents}%</show>
</balloon>
<legend>
<enabled/>
<max_columns>1</max_columns>
<x>400</x>
<y>100</y>
<width>300</width>
<values>
<enabled>true</enabled>
<width>100</width>
</values>
</legend>
<labels>
<label>
<x>0</x>
<y>20</y>
<align>center</align>
<text_size>14</text_size>
<text>
<b>amCharts.com visitors' countries</b>
</text>
</label>
</labels>
</settings>